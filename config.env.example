# Environment Configuration for Log Analysis System

# JWT Secret Key (change in production)
JWT_SECRET=your-secret-key-change-in-production

# Elasticsearch Configuration
ELASTICSEARCH_URL=http://localhost:9201

# GenAPI Configuration
GENAPI_KEY=your-genapi-key-here
GENAPI_URL=https://api.genapi.com/v1

# Gemini API Configuration
GEMINI_API_KEY=your-gemini-api-key-here

# Application Configuration
FLASK_ENV=development
FLASK_DEBUG=True
MAX_CONTENT_LENGTH=104857600  # 100MB

# Log Processing Configuration (speed-optimized defaults)
FAST_MODE=true
CHUNK_SIZE=80
ERROR_WINDOW_SIZE=1
SIMILARITY_THRESHOLD=0.7
SKIP_CHUNK_EMBEDDINGS=true
EMBEDDING_DIMS=384

# Security Configuration
BCRYPT_ROUNDS=12
TOKEN_EXPIRY_HOURS=24
